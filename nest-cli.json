{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include":"**/*.html",
        "outDir":"dist/src"
      },
      {
        "include":"test/keys/**/*",
        "outDir":"dist/src"
      }
    ]
  }
}
